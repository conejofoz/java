package entities;

public class Produto {
	
	public String nome;
	public double preco;
	public int quantidade;
	
	
	public double estoqueTotal() {
		return quantidade * preco;
	}
	
	public void adicionarProduto(int qtd) {
		quantidade += qtd;
	}
	
	public void removerProduto(int qtd) {
		quantidade -= qtd;
	}

	@Override
	public String toString() {
		
		return nome ;
	}
	
	
	

}
